\ProvidesPackage{cu-num-test}[2016/03/03 v1.0 CU Numerics (Testing)]
\RequirePackage{cu-num}
%
% Test API
%
\def\cunumtest#1#2{%
  \cu@fmt@{#1}%
  \def\@y{#2}%
  \ifx\@x\@y\else
    \errmessage{TEST FAILED: #1: "\@x" expected "#2"}%
  \fi
}%
